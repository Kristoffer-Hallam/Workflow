   "source": [
    "# Fibonacci Generator\n",
    "def fib(num):\n",
    "    a,b = 0,1\n",
    "    for i in xrange(0,num):\n",
    "        yield \"{} : {}\".format(i+1,a)\n",
    "        a, b = b, a+b\n",
    "        \n",
    "for item in fib(10):\n",
    "    print item"
   ]
